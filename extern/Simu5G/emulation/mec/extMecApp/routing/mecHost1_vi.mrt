ifconfig:

# interface to the upf_mec
name: pppIf0
    inet_addr: 10.0.5.2
    Mask: 255.255.255.0
    MTU: 1500
    Metric: 1
    POINTTOPOINT MULTICAST


# external interface
name: extEth0
    inet_addr: 192.168.2.1
    Mask: 255.255.255.0
    MTU: 1500
    POINTTOPOINT MULTICAST


ifconfigend.

route:

#Destination     Gateway          Genmask          Flags  Metric  Iface
192.168.2.0      *                255.255.255.0    H      0       extEth0
0.0.0.0          *                0.0.0.0          G      0       pppIf0

routeend.


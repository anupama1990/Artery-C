ifconfig:

# interface to the external client
name: eth0  
    inet_addr: 192.168.3.1
    Mask: 255.255.255.0
    MTU: 1500
    Metric: 1
    POINTTOPOINT MULTICAST
    
name: cellular
    inet_addr: 10.0.0.1
    Mask: 255.255.255.0
    MTU: 1500
    POINTTOPOINT MULTICAST
    
ifconfigend.

route:
#Destination     Gateway          Genmask          Flags  Metric  Iface
192.168.3.0      *                255.255.255.0    H      0       eth0
0.0.0.0          *                0.0.0.0          G      0       cellular

routeend.

